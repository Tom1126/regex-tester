{
  "manifest_version": 1,
  "name": "Regex Tester",
  "version": "1.1.1",
  "description": "Tests regular expressions against strings of texts.",
  "homepage_url": "https://github.com/Tom1126/regex-tester",
  "icons": {
    "32": "images/icon_32.svg",
    "48": "images/icon_32.svg",
    "96": "images/icon_32.svg"
  },
  "applications": {
    "gecko": {
      "id": "@regextester",
      "strict_min_version": "50.0"
    }
  },
  "permissions": ["storage"],
  "browser_action": {
    "default_icon": "images/icon_32.svg",
    "default_title": "Regex Tester",
    "default_popup": "addon/popup/index.html"
  }
}